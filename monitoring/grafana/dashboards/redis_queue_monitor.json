{
  "dashboard": {
    "title": "Redis Queue & Data Collection Monitor",
    "tags": ["redis", "queue", "realtime", "monitoring"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "templating": {
      "list": [
        {
          "name": "exchange",
          "type": "query",
          "label": "Exchange",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "query": {
            "query": "label_values(ws_collector_connection_status, exchange)",
            "refId": "PrometheusVariableQueryEditor-VariableQuery"
          },
          "current": {
            "text": "bybit",
            "value": "bybit"
          },
          "includeAll": false,
          "multi": false,
          "refresh": 1
        },
        {
          "name": "symbol",
          "type": "query",
          "label": "Symbol",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "query": {
            "query": "label_values(ws_collector_trades_collected_total{exchange=\"$exchange\"}, symbol)",
            "refId": "PrometheusVariableQueryEditor-VariableQuery"
          },
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true,
          "refresh": 1
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "ðŸ“Š System Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "WebSocket Connection",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_connection_status{exchange=\"$exchange\"}",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Disconnected",
                    "color": "red"
                  },
                  "1": {
                    "text": "Connected",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "none",
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "REST Collector Status",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "collector_running",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Stopped",
                    "color": "red"
                  },
                  "1": {
                    "text": "Running",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none"
        }
      },
      {
        "id": 4,
        "title": "WS Uptime",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_uptime_seconds",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 1},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 60, "color": "yellow"},
                {"value": 300, "color": "green"}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "title": "Total WS Messages",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "sum(ws_collector_messages_total{exchange=\"$exchange\"})",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "graphMode": "area"
        }
      },
      {
        "id": 6,
        "title": "Total Trades Collected",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "sum(ws_collector_trades_collected_total{exchange=\"$exchange\", symbol=~\"$symbol\"})",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "graphMode": "area"
        }
      },
      {
        "id": 10,
        "title": "ðŸ“¦ Redis Queue Status",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Trade Queue Size",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_redis_queue_size{queue_type=\"trade\"}",
            "legendFormat": "Trade Queue",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 12,
        "title": "OrderBook Snapshot Queue Size",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_redis_queue_size{queue_type=\"orderbook_snapshot\"}",
            "legendFormat": "OrderBook Queue",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 14,
        "title": "Redis Queue Size Overview",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_redis_queue_size{queue_type=\"trade\"}",
            "legendFormat": "Trade",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          },
          {
            "expr": "ws_collector_redis_queue_size{queue_type=\"orderbook_snapshot\"}",
            "legendFormat": "OrderBook",
            "refId": "B",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 1000, "color": "red"}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 31,
        "title": "DB Flush Status",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_db_is_flushing",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Idle",
                    "color": "green"
                  },
                  "1": {
                    "text": "Flushing",
                    "color": "blue"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "blue"}
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none"
        }
      },
      {
        "id": 20,
        "title": "ðŸ“ˆ Data Collection Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 22},
        "collapsed": false
      },
      {
        "id": 21,
        "title": "Trade Collection Rate",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "rate(ws_collector_trades_collected_total{exchange=\"$exchange\", symbol=~\"$symbol\"}[1m]) * 60",
            "legendFormat": "{{symbol}}",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 23},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 41,
        "title": "Best Bid/Ask Prices",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "ws_collector_orderbook_best_bid_price{exchange=\"$exchange\", symbol=~\"$symbol\"}",
            "legendFormat": "{{symbol}} Bid",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          },
          {
            "expr": "ws_collector_orderbook_best_ask_price{exchange=\"$exchange\", symbol=~\"$symbol\"}",
            "legendFormat": "{{symbol}} Ask",
            "refId": "B",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 23},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 0,
              "showPoints": "never"
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyUSD"
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      }
    ]
  },
  "overwrite": true
}
